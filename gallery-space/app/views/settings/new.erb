<h1>Make new settings</h1>
<%=form_for(@setting) do |f|%>
<%=f.label :theme_name%><br>
<%=f.text_field :theme_name%><br>
<%=f.label :background_color%><br>
<%=f.text_field :background_color%><br>
<%=f.label :font%><br>
<%=f.text_field :font%><br>
<%=f.label :audio%><br>
<%=f.text_field :audio%><br>
<%=f.submit%>
<%end%>
